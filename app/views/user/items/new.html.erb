<div class="booyah-box col-10 offset-1">
  <h1>Add a new item</h1>
  <%= simple_form_for @item, url: user_items_path do |f| %>
    <%= f.input :title %>
    <%= f.input :cost %>
    <br />
    <%= f.submit "create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>